{
  "name": "ryoa-mono",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "pnpm --filter \"./apps/**\" dev",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docs:dev": "pnpm --filter docs dev",
    "app:dev": "pnpm --filter rollyourownauth dev"
  },
  "devDependencies": {
    "@turbo/gen": "^1.12.4",
    "prettier": "^3.1.1",
    "turbo": "^1.12.4"
  },
  "packageManager": "pnpm@8.9.0"
}
